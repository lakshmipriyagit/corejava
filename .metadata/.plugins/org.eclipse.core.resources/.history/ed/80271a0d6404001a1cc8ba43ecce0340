
public class string {
public static void main(String[] args) {
	String s1="hello";
	String s2=new String("hello");
	if(s1==s2)
		
	System.out.println("both objects are equal");
	else
		System.out.println("both objects are not equal");
	if(s1.equals(s2))
	{
		System.out.println("both objects are equal");
	}
	else
		System.out.println("both objects are not equal");
}
}
