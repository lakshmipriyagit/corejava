
public class throw1 
{
	void disp(String a,int b)throws nameage
	{
		if(b>=18&&b<60)
		{
			throw new nameage("the age is matched"+a+" "+b);
		}
		else
		{
			System.out.println("age that entered is ");
		}
	}
public static void main(String[] args) throws nameage
{
throw1 ob=new throw1();
ob.disp("lucky", 18);
}
}
