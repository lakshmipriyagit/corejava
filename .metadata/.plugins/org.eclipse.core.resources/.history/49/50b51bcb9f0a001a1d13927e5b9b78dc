import java.util.Scanner;
public class x {
		public static void main(String[] args) {
			int a[]=new int[5];int sum=0;float avg;
			x ob=new x();
			Scanner sc=new Scanner(System.in);
		for(int i=0;i<5;i++)
		{
			a[i]=sc.nextInt();
			if(a[i]>2)
			{
			sum=sum+a[i];
			ob.disp();
			}
			else
				System.out.println("No");
			break;
		}
		avg=sum/5;
		
		}
}
